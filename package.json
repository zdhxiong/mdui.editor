{
  "name": "mdui.editor",
  "version": "1.0.2",
  "description": "Material Design 样式的富文本编辑器",
  "main": "dist/js/editor.js",
  "module": "dist/js/editor.esm.js",
  "types": "es/index.d.ts",
  "style": "dist/css/editor.css",
  "less": "src/index.less",
  "scripts": {
    "eslint": "eslint --fix src/**/*.ts",
    "stylelint": "stylelint --fix src/**/*.less",
    "lint": "npm run eslint && npm run stylelint",
    "js": "npm run eslint && tsc -p src/tsconfig.json && rollup --config",
    "css": "npm run stylelint && gulp",
    "build": "npm run js && npm run css",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist",
    "es",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zdhxiong/mdui.editor.git"
  },
  "keywords": [
    "mdui",
    "editor",
    "material design"
  ],
  "author": "zdhxiong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zdhxiong/mdui.editor/issues",
    "email": "zdhxiong@gmail.com"
  },
  "homepage": "https://github.com/zdhxiong/mdui.editor#readme",
  "dependencies": {
    "mdn-polyfills": "^5.20.0",
    "mdui": "^1.0.1",
    "mdui.jq": "^2.0.1",
    "normalize.css": "^8.0.1",
    "promise-polyfill": "^8.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-header": "^2.0.9",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^5.0.9",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-polyfill": "^3.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "engines": {
    "node": ">=14"
  }
}
